# Copy to iac/instances/<instance-name>.env and customize for that instance.
# This file is intentionally example-only. Do not commit real instance env files.

SUBSCRIPTION_ID=00000000-0000-0000-0000-000000000000
TENANT_ID=00000000-0000-0000-0000-000000000000

# Optional. If omitted, deploy-instance.sh uses <instance-name>.
ENV_NAME=app1-dev
LOCATION=eastus

# Optional TF_VAR overrides for this instance:
TF_VAR_tags={"project":"auth-platform","instance":"app1-dev","owner":"platform-team"}
TF_VAR_postgres_admin_username=authpgadmin
TF_VAR_postgres_sku=B_Standard_B1ms
TF_VAR_postgres_storage_mb=32768
TF_VAR_postgres_public_network_access_enabled=true
TF_VAR_allow_azure_services=true
TF_VAR_postgres_allowed_ips=[]
TF_VAR_redis_sku=Basic
TF_VAR_redis_capacity=0
TF_VAR_enable_container_apps_env=true
TF_VAR_enable_storage_account=true
